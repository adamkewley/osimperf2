#!/bin/bash
set -eo pipefail

WORKSPACE=$(dirname $(realpath "$0"))

echo "Setup Arm26 benchmark at $WORKSPACE"

models_dir=$(realpath $WORKSPACE/../../opensim-models)
echo "Copy models from $models_dir"

cp -r $models_dir/Geometry $WORKSPACE/Geometry

binary=ToolPerf
results=$WORKSPACE/benchmark-result_Shoulder.csv
log=$WORKSPACE/log.txt

echo "Run shoulder model"

$binary --model TestShoulderWrapping.osim --out $results \
	--run "0.5"

# opensim-cmd run-tool forwardTool.xml
